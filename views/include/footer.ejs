        img.alt = 'Image not found';
    }
</script>
<script>
    // Get the current URL
    let currentUrl = window.location.pathname;

    // Get all navigation links
    const navLinks = document.querySelectorAll('.nav-link');


    // Loop through each navigation link
    navLinks.forEach(link => {
        // Check if the link's href matches the current URL
        if (link.getAttribute('href') === currentUrl) {
            // Add the 'active' class to the matching link
            link.classList.add('active');
        }
    });

    const Links = document.querySelectorAll('.mobile-menu li a');


    // Loop through each navigation link
    Links.forEach(link => {
        // Check if the link's href matches the current URL
        if (link.getAttribute('href') === currentUrl) {
            // Add the 'active' class to the matching link
            link.classList.add('active');
        }
    });
</script>




<script>
    (function () {
        'use strict'
        const forms = document.querySelectorAll('.requires-validation')
        Array.from(forms)
            .forEach(function (form) {
                form.addEventListener('submit', function (event) {
                    if (!form.checkValidity()) {
                        event.preventDefault()
                        event.stopPropagation()
                    }

                    form.classList.add('was-validated')
                }, false)
            })
    })()
</script>

<script>
    $(document).ready(function () {
        var table = $('#example').DataTable({
            scrollX: true,
            paging: false, // Disable pagination
            searching: false, // Disable search
        });

        // Check the number of rows in the DataTable
        var rowCount = table.rows().count();

        // Modify the DataTable's options based on the number of rows
        var options = {
            scrollX: true,
            paging: false,
            searching: false,
        };

        if (rowCount > 10) {
            options.scrollY = 400; // Set fixed height
        } else {
            options.scrollY = 'auto'; // Set auto height
        }

        // Reinitialize the DataTable with the modified options
        table.destroy(); // Destroy the existing DataTable
        table = $('#example').DataTable(options); // Reinitialize DataTable
    });
</script>



